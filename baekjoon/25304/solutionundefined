// [문제 링크]: https://www.acmicpc.net/problem/25304

#include <iostream>
​
using namespace std;
​
int main()
{
​
    int TotalPrice = 0;
    int Item = 0;
    int Price = 0;
    int Count = 0;
    int PricePlus = 0;
​
    cin >> TotalPrice;
    cin >> Item;
​
​
    for (int i = 0; i < Item; i++)
    {
        cin >> Price;  cin >> Count;
​
        PricePlus += Price * Count;
​
    }
​
    if (PricePlus == TotalPrice)
    {
        cout << "Yes" << endl;
    }
    else
    {
        cout << "No" << endl;
    }
​
​
    return 0;
}